<%--GridViews--%>
<asp:GridView runat="server" CellPadding="5" EmptyDataText="رکوردی جهت نمایش وجود ندارد"
  HorizontalAlign="Center"  Font-Names="BRoya" ForeColor="#333333" GridLines="None" Font-Bold="True" PageSize="5" Font-Size="12" CssClass="gridcontainer" >   
                    <AlternatingRowStyle BackColor="#EBEBEB" ForeColor="#333333" />
                    <EditRowStyle BackColor="#999999" />
                    <FooterStyle BackColor="#096B95" ForeColor="White" />
                    <HeaderStyle  BackColor="#2ab0e9" ForeColor="White"  HorizontalAlign="Center" VerticalAlign="Middle" />
                    <PagerStyle BackColor="#e4e4e4" Font-Size="10"  Height="20px" ForeColor="#6a6a6a" HorizontalAlign="Center" />
                    <RowStyle BackColor="White" ForeColor="#284775"  HorizontalAlign="Center" VerticalAlign="Middle" />
                    <SelectedRowStyle BackColor="#E2DED6" ForeColor="#333333" HorizontalAlign="Center" VerticalAlign="Middle" />                 
                    <SortedAscendingHeaderStyle BackColor="#096B95" ForeColor="White"/>
                    <SortedDescendingHeaderStyle BackColor="#096B95" ForeColor="White"/>
</asp:GridView>

<%--FormView--%>
