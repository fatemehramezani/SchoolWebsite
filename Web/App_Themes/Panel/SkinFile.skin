<%--PersianDateTextBox--%>
<pdc:PersianDateTextBox runat="server" PickerDirection="ToRight" ShowPickerOnEvent="None" ShowPickerOnTop="True" IconUrl="images/booking/calendar-icon.png"/>

<%--PersianDateScriptManager--%>
<pdc:PersianDateScriptManager runat="server"  CalendarCSS="PickerCalendarCSS" FooterCSS="PickerFooterCSS" ForbidenCSS="PickerForbidenCSS" FrameCSS="PickerCSS" HeaderCSS="PickerHeaderCSS" SelectedCSS="PickerSelectedCSS" WeekDayCSS="PickerWeekDayCSS" WorkDayCSS="PickerWorkDayCSS" WeekDaysName="'ش','ی','د','س','چ','پ','ج'" CalendarDayWidth="30"/>

