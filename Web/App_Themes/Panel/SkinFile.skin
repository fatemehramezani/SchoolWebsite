    <FooterStyle BackColor="#2ab0e9" Font-Bold="True" ForeColor="#003282" />
    <EditRowStyle BackColor="#e4e4e4" />
    <InsertRowStyle BackColor="#e4e4e4" />
    <RowStyle BackColor="#f8f8f8" />
    <PagerStyle BackColor="#2ab0e9" ForeColor="003282" HorizontalAlign="Center" />
    <HeaderStyle BackColor="#2ab0e9" Font-Bold="True" ForeColor="White" />
</asp:FormView>

